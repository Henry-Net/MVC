
        @Model AdminLog

        <h1>@ViewBag.loginstate</h1>
        <div>
            @using (Html.BeginForm("login", "Account", FormMethod.Post))
            {
                <form class="form-inline">
                    <div class="form-group">
                        <label for="exampleInputName2">Name</label>
                        <input type="text" class="form-control" id="exampleInputName" name="exampleInputName" placeholder="Jane Doe">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail2">Email</label>
                        <input type="email" class="form-control" id="exampleInputEmail" name="exampleInputEmail" placeholder="jane.doe@example.com">
                    </div>
                    <button type="submit" class="btn btn-default">Send invitation</button>
                </form>
            }
        </div>
        <div>
            <table>
                
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.UserName</td>
                        </tr>
                    }
            </table>

        </div>

